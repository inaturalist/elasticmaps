{
  "env": {
    "node": true,
    "mocha": true,
    "es6": true
  },
  "parser": "babel-eslint",
  "extends": "airbnb",
  "rules": {
    "arrow-parens": [2, "as-needed"],
    "comma-dangle": [2, "never"],
    "consistent-return": [2, { "treatUndefinedAsUnspecified": true }],
    "func-names": 0,
    "no-void": 0,
    "prefer-destructuring": [2, { "object": true, "array": false }],
    "quotes": [2, "double"],
    "space-in-parens": [2, "always"]
  }
}
