{
  "env": {
    "node": true,
    "mocha": true,
    "es6": true,
    "browser": true
  },
  "parser": "babel-eslint",
  "extends": "airbnb",
  "rules": {
    "quotes": [2, "double"],
    "comma-dangle": [2, "never"],
    "space-in-parens": [2, "always"],
    "func-names": 0,
    "arrow-parens": [2, "as-needed"]
  }
}
